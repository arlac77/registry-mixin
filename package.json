{
  "name": "registry-mixin",
  "version": "0.0.0-semantic-release",
  "description": "Register objects and factories for later lookup",
  "main": "dist/RegistryMixin.js",
  "module": "src/RegistryMixin.js",
  "keywords": [
    "lookup",
    "registry",
    "mixin"
  ],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/RegistryMixin.js -c -- src/RegistryMixin.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/registry-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/registry-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/registry-mixin#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.0.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}