{
  "name": "registry-mixin",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/registry-mixin.mjs",
  "exports": {
    ".": "./src/registry-mixin.mjs"
  },
  "description": "Register objects and factories for later lookup",
  "keywords": [
    "lookup",
    "mixin",
    "registry"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/registry-mixin.mjs --section=API && documentation readme ./src/registry-mixin.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/registry-mixin.mjs && documentation lint ./src/registry-mixin.mjs"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.2",
    "documentation": "^13.0.2",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/registry-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/registry-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/registry-mixin#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
