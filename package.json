{
  "name": "registry-mixin",
  "version": "0.0.0-semantic-release",
  "description": "Register objects and factories for later lookup",
  "main": "dist/RegistryMixin.js",
  "jsnext:main": "src/RegistryMixin.js",
  "module": "src/RegistryMixin.js",
  "keywords": [
    "lookup",
    "registry",
    "mixin"
  ],
  "scripts": {
    "cover": "npm run build &&  ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "jsdoc src/*.js",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/RegistryMixin.js -- src/RegistryMixin.js",
    "test": "npm run build && mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/registry-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/registry-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/registry-mixin#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "3.4.1",
    "mocha": "3.1.0",
    "rollup": "^0.36.1",
    "semantic-release": "^6.3.1"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
