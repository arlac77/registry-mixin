{
  "name": "registry-mixin",
  "version": "0.0.0-semantic-release",
  "description": "Register objects and factories for later lookup",
  "main": "dist/RegistryMixin.js",
  "module": "src/RegistryMixin.js",
  "keywords": [
    "lookup",
    "registry",
    "mixin"
  ],
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "build": "node_modules/rollup/bin/rollup --output=dist/RegistryMixin.js -c -- src/RegistryMixin.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/registry-mixin.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/registry-mixin/issues"
  },
  "homepage": "https://github.com/arlac77/registry-mixin#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "rollup": "^0.37.0",
    "semantic-release": "^6.3.3",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}